
.header-show{
	height: 80rpx;
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	-webkit-box-pack: center;
	-webkit-justify-content: center;
	        justify-content: center;
	-webkit-box-align: center;
	-webkit-align-items: center;
	        align-items: center;
	padding: 20rpx;
}
.search{
	position: relative;
	-webkit-box-flex: 8;
	-webkit-flex: 8;
	        flex: 8;
	height: 100%;
	background-color: #f1f2f6;
	-webkit-border-radius: 5rpx;
	        border-radius: 5rpx;
}
.search input{
	width: 100%;
	height: 100%;
	padding:0rpx 20rpx 0 70rpx ;
	-webkit-box-sizing: border-box;
	        box-sizing: border-box;
	-webkit-border-radius: 10rpx;
	        border-radius: 10rpx;
}
.icon-search{
	display: block;
	position: absolute;
	left: 18rpx;
	top: 18rpx;
	width: 40rpx;
	height: 40rpx;
	background: url(data:image/png;base64,);
	background-size: 100% 100%;
}
.icon-gw{
	display: block;
	width: 50rpx;
	height: 50rpx;
	margin-left: 65rpx;
	background: url(data:image/png;base64,);
	background-size: 100% 100%;
}
.change{
	-webkit-box-flex: 2;
	-webkit-flex: 2;
	        flex: 2;
	text-align: right;
	font-size: 22rpx;
	color: #4CD964;
}
.swiper-ad{
	padding: 20rpx;
}
.swiper-ad swiper-item{
	-webkit-border-radius: 20rpx;
	        border-radius: 20rpx;
}
.swiper-ad .swiper-item{
	width: 100%;
	height: 100%;
	-webkit-border-radius: 20rpx;
	        border-radius: 20rpx;
}
.swiper-ad swiper-item ._img{
	width: 100%;
	height: 100%;
}
.class-nav{
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	-webkit-box-align: center;
	-webkit-align-items: center;
	        align-items: center;
	-webkit-box-pack: center;
	-webkit-justify-content: center;
	        justify-content: center;
	-webkit-flex-wrap:wrap;
	        flex-wrap:wrap;
	padding-bottom: 40rpx;
	border-bottom: 1rpx solid #eee;
}
.class-nav view{
	-webkit-box-flex: 20%;
	-webkit-flex: 20%;
	        flex: 20%;
	text-align: center;
	font-size: 24rpx;
	margin-top: 20rpx;
}
.class-nav view ._p ._img{
	width: 80rpx;
	height: 80rpx;
	-webkit-border-radius: 50%;
	        border-radius: 50%;
}
.notice{
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	-webkit-box-align: center;
	-webkit-align-items: center;
	        align-items: center;
	-webkit-box-pack: center;
	-webkit-justify-content: center;
	        justify-content: center;
	border-bottom: 20rpx solid #f1f2f6;
}
.notice .title{
	-webkit-box-flex: 25%;
	-webkit-flex: 25%;
	        flex: 25%;
	text-align: center;
	border-right: 1rpx solid #eee;
}
.notice .title ._img{
	width:70%;
}
.notice .center{
	-webkit-box-flex: 65%;
	-webkit-flex: 65%;
	        flex: 65%;
}
.notice .more{
	-webkit-box-flex: 10%;
	-webkit-flex: 10%;
	        flex: 10%;
	border-left: 1rpx solid #eee;
	font-size: 24rpx;
	color: #666;
	padding-left: 20rpx;
}
.notice .center .swiper-notice{
	height: 80rpx;
	line-height: 80rpx;
	width: 90%;
	margin: 0 auto;
}
.notice .center .swiper-notice swiper-item{
	height: 100%;
	width: 100%;
}
.notice .center .swiper-notice .swiper-item{
	height: 100%;
	width: 100%;
	overflow: hidden;
	text-overflow:ellipsis;
	white-space: nowrap;
	font-size: 24rpx;
}
.post{
	padding: 20rpx;
}
.post .nav{
	width:100%;
	margin: 0 auto;
	border-bottom: 1rpx solid #eee;
	font-size: 28rpx;
	color: #666;		
	padding-bottom: 10rpx;
}
.post .nav ._span{
	margin-right: 20rpx;
	padding-bottom: 10rpx;
}
.post .nav ._span.act{
	color: #000;
	border-bottom: 4rpx solid #4CD964;
}
.copy{
	width: 100%;
	height: 200rpx;
	text-align: center;
	line-height: 200rpx;
	font-size: 24rpx;
	background: #f1f2f6;
	color: #666;
}

